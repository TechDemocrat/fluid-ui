        "build-storybook": "build-storybook",
        "stylelint:fix": "stylelint \"**/*.scss\" --fix",
        "lint:watch": "tsc --watch --noEmit --emitDeclarationOnly false && eslint-watch src --max-warnings 0",
        "lint": "tsc --noEmit --emitDeclarationOnly false && eslint src --max-warnings 0",
        "lint:fix": "eslint src/**/*.ts{,x} --fix --max-warnings 0",
        "prettier": "prettier --write src/**/*.ts{,x}",
        "lint-staged": "lint-staged",
        "husky:install": "husky install",
        "prepare": "yarn husky:install",
        "husky:pre-commit": "yarn lint-staged",
        "chromatic": "chromatic --exit-zero-on-changes",
        "fui": "node scripts/fui-cli.js"
    },
